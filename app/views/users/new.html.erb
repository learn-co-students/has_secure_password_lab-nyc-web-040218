<%= form_for @user do |t| %>
    <%= t.label :name %>
    <%= t.text_field :name %><br>
    <%= t.label :username %>
    <%= t.text_field :username %><br>
    <%= t.label :password %>
    <%= t.password_field :password %><br>
    <%= t.label :password_confirmation %>
    <%= t.password_field :password_confirmation %><br>
    <%= t.submit %>
<% end %>